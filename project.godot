; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Resource",
"class": "AudioTimeEvent",
"language": "GDScript",
"path": "res://resources/scripts/audio_time_event.gd"
} ]
_global_script_class_icons={
"AudioTimeEvent": ""
}

[application]

config/name="Index Project Dev-Blog"
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://icon.png"

[audio]

default_bus_layout="res://default_audio.tres"

[autoload]

Video="*res://resources/scripts/video.gd"

[debug]

settings/fps/force_fps=90

[display]

window/size/width=1920
window/size/height=1080
window/size/test_width=1280
window/size/test_height=720
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[gui]

common/drop_mouse_on_gui_input_disabled=true

[importer_defaults]

mp3={
"loop": false,
"loop_offset": 0
}

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_clear_color=Color( 0, 0, 0, 1 )
environment/default_environment="res://default_env.tres"
